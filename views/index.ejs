		<div class="jumbotron">
			<h3>Upload image</h3>
		
			<form class="form-signin" id="upload-image" name="upload-image" enctype="multipart/form-data" method="post" action="/upload">
				<input class="form-control" type="text" id="fname" name="fname" placeholder="First name"/><br/>
				<input class="form-control" type="text" id="lname" name="lname" placeholder="Last name"/><br/>
				<input class="form-control" type="text" id="dob" name="dob" placeholder="Date of birth (MMDDYYYY)"/><br/>			
				<input class="form-control" type="file" id="imagefile" name="imagefile"/><br/>
				<input class="form-control" type="text" id="dest" name="dest" placeholder="File destination"/><br/>
				<input class="form-control" type="submit" id="submit" value="Upload file"/><br>
			</form>
